{
  "name": "Image Alt",
  "version": "1.0",
  "description": "displays alt text of an image",
  "manifest_version": 2,
  "icons": {
  	"128":"ariaicon.png"
  },
  "browser_action": {
  	"default_title": "See alt text"
  },
  
  "permissions": [
  "http://*/*/", "https://*/*/"
  ],

  "content_scripts": [{
    "css": ["styles.css"],
    "js": ["myscript.js", "jquery-3.3.1.js"],
    "matches": ["<all_urls>"]
  }]
}
